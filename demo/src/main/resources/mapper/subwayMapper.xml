<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.example.demo.DAO.subwayDAO">

<!-- 노선 정보 가져오기 -->
<select id="selectLine" resultType="com.example.demo.VO.lineVO">
	select * from line
</select>

<!-- 하차역 정보 가져오기 -->
<select id="selectStation" parameterType="_int" resultType="com.example.demo.VO.stationVO">
	select * from station where lineNum=#{lineNum}
</select>

<!-- 하차정보 등록하기 -->
<insert id="insertGetoffInfo" parameterType="com.example.demo.VO.getoff_infoVO">
</insert>

</mapper>
